{
  "name": "metadata-directus",
  "displayName": "Metadata Provider - Directus",
  "author": "Colin Berry",
  "version": "1.0.0",
  "launcherVersion": "10.2.0",
  "description": "Allows use of a Directus host to fetch live metadata",
  "icon": "icon.png",
  "main": "./dist/ext.js",
  "contributes": {
    "metadataProviders": [
      {
        "id": "directus-provider",
        "name": "Directus",
        "validateCommand": "metadata-directus.validate-provider-instance",
        "createCommand": "metadata-directus.create-provider-instance"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "14.10.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-zip": "^4.2.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "package": "gulp",
    "build": "webpack --mode development",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
